{
  "name": "sistema-analise-oleo-server",
  "version": "1.0.0",
  "description": "Sistema de análises de óleo (Desktop)",
  "main": "dist/bootstrap.js",
  "homepage": "./",
  "scripts": {
    "dev": "tsx src/bootstrap.ts",
    "dev:watch": "tsc -w",
    "build": "tsc",
    "start": "node dist/bootstrap.js",
    "init-db": "tsx src/initDB.ts",
    "build:exe": "npm run build && npx pkg dist/bootstrap.js --targets node18-win-x64 --output ../dist/sistema.exe"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.88.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^6.4.14",
    "@types/uuid": "^9.0.7",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "build": {}
}

